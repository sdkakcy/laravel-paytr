{
    "name": "gizemsever/laravel-paytr",
    "description": "Laravel Paytr Wrapper",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gizem Sever",
            "email": "gizemsever68@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/collections": "^8|^9|^10|^11|^12",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "dev-main"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Gizemsever\\LaravelPaytr\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "format:test": "vendor/bin/pint --test"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gizemsever\\LaravelPaytr\\PaytrServiceProvider"
            ],
            "aliases": {
                "Paytr": "Gizemsever\\LaravelPaytr\\PaytrFacade"
            }
        }
    }
}
